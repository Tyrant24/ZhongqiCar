/* 淡入淡出 */
.mask{
  width: 100%;
  height: 100%;
  position: fixed;
  top: 0;
  left: 0;
  background: #000;
  z-index: 9000;
  opacity: 0;
}
.modalDlg{
  width: 560rpx;
  /* height: 400rpx; */
  position: fixed;
  top: 60%;
  left: 0;
  z-index: 9999;
  margin: -370rpx 85rpx;
  background-color: #fff;
  border-radius: 10rpx; 
  overflow: hidden; 
}

